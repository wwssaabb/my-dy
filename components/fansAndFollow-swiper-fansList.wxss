
.fans-and-follow-swiper-fans-list{
	width: 100%;
	height: calc(100vh - 95px);
}
.fans-and-follow-swiper-fans-list-empty{
	width: 100%;
	height: auto;
	margin: 80px 0 0 50%;
	-webkit-transform: translateX(-50%);
	        transform: translateX(-50%);
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;	
	-webkit-box-align: center;	
	-webkit-align-items: center;	
	        align-items: center;
}
.fans-and-follow-swiper-fans-list-empty-img{
	width: 50vw;
	height: 33.33333vw;
	margin-bottom: -20px;
}
.fans-and-follow-swiper-fans-list-content{
	width: 100%;
	height: auto;
}

