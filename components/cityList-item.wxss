
.city-list-item{
	width: 100%;
	height: auto;
	padding: 5px 10px;
}
.city-list-item-title{
	width: 100%;
	height: auto;
	font-size: 17px;
	font-weight: bold;
	color: #fff;
	margin: 5px 0;
}
.city-list-item-content{
	width: 100%;
	height: auto;
}
.city-list-item-title-sticky{
	display: inline-block;
	position: -webkit-sticky;
	position: sticky;
	top: 0;
	left: 0;
	padding: 5px 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	background-color:#161823 ;
	border-bottom: 1px solid #393a44;
	z-index: 21;
}

