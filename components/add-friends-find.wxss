
.add-friends-content-find-wrap{
	width: 100%;
	height: 100%;
}
.add-friends-find-search-wrap{
	width: 90%;
	height: auto;
	left: 5%;
	position: relative;
	background-color:#161823 ;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	margin: 10px 0;
}
.add-friends-find-search{
	width: 100%;
	height: 30px;
	background-color:#161823 ;
	color: #fff;
}
.add-friends-find-search-scan-icon{
	position: absolute;
	right: 15px;
	top:13px;
	font-size: 20px;
	font-weight: bold;
	color: #6b6b73;
}
.add-friends-find-address{
	width: 90%;
	height: 70px;
	margin: 20px 0 10px;
	margin-left: 50%;
	-webkit-transform: translateX(-50%);
	        transform: translateX(-50%);
	border-bottom:2px solid  #272833;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	        justify-content: flex-start;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.add-friends-find-address-icon-wrap{
	width: 50px;
	height: 50px;
	position: relative;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	background-color: #242630;
	margin-right:10px ;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.add-friends-find-address-icon{
	font-size: 25px;
	font-weight: normal;
	color: #c9c9cc;
}
.add-friends-find-address-text-wrap{
	width: auto;
	height: 50px;
	color: #808086;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: start;
	-webkit-align-items: flex-start;
	        align-items: flex-start;
}
.add-friends-find-address-text1{
	font-size: 15px;
	font-weight: bold;
	color: #e0e0e2;
}
.add-friends-find-address-text2{
	font-size: 14px;
	font-weight: bold;
}
.add-friends-find-address-arrow-icon{
	position: absolute;
	font-size: 18px;
	font-weight: bold;
	color:#898a90 ;
	right:5px ;
}
.add-friend-recommend{
	width: 90%;
	height: auto;
	margin-left: 50%;
	-webkit-transform: translateX(-50%);
	        transform: translateX(-50%);
	overflow: hidden;
}

