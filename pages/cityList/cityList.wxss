
.city-list{
	width: 100%;
	height: 100vh;
	position: fixed;
	top:0;
	left: 0;
	padding-top:30px;
	background-color: #161823;
	overflow: hidden;
}
.city-list-top{
	width: 100%;
	height: 90px;
	overflow: hidden;
	z-index: 35;
	margin: 0;
}
.back-city{
	position: fixed;
	top: 25px;
	left: 0;
	width: 25px;
	height: 25px;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.back-city-icon{
	width: 30px;
	height: 30px;
	margin:20px 0 0 20px;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 24px;
	line-height: 30px;
	color: #C0C0C0;
	-webkit-transform: rotate(45deg);
	        transform: rotate(45deg);
	z-index: 21;
}
.city-list-title{
	display: block;
	width: 100%;
	font-size: 16px;
	font-style: 宋体;
	font-weight: bold;
	color: #FFF;
	text-align: center;
	margin-top:10px;
}
.city-list-search-wrap{
	width: 100%;
	height: 45px;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	background-color: #161823;
	margin-bottom:10px;
}
.uni-searchbar.uni-searchbar.data-v-64ee3838.data-v-64ee3838 {
	background-color: #161823;
}
.uni-searchbar__cancel.uni-searchbar__cancel.data-v-64ee3838.data-v-64ee3838 {
	background-color: #d12026;
	color: #fff;
	font-weight: bold;
	padding:0 4px;
	margin:0 4px;
	-webkit-border-radius: 5px;
	        border-radius: 5px;
}
.uni-searchbar__box-search-input.uni-searchbar__box-search-input.data-v-64ee3838.data-v-64ee3838{
	color: #C0C0C0;
}
.city-list-content{
	width: calc(100vw - 30px);
	height: calc(100vh - 115px);
	margin:0 20px 0 10px;
}
.city-letter{
	position: fixed;
	top: 25px;
	right: 5px;
	width: 15px;
	height: calc(100vh - 25px);
	z-index: 21px;
}

