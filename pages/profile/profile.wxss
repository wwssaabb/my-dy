
.profile-container{
	width: 140vw;
	height: 100vh;
	position: relative;
	-webkit-transform: translateX(0);
	        transform: translateX(0);
	-webkit-transition-timing-function:cubic-bezier(.42,0,0.58,1);
	        transition-timing-function:cubic-bezier(.42,0,0.58,1);
	-webkit-transition: .3s;
	transition: .3s;
	overflow: hidden;
}
