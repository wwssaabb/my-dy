
.city{
	width: 100%;
	height: 100%;
	background-color: #161823;
}
.city-nav{
	position:fixed;
	top:25px;
	left: 0;
	right: 0;
	height: 35px;
	width: 100%;
	z-index: 20;
}
.city-tab{
	position:fixed;
	bottom:0;
	left: 0;
	right: 0;
	height: 50px;
	width: 100%;
	z-index: 20;
}
.city-content{
	width: 100%;
	height: calc(100vh - 110px);
	position: fixed;
	top: 60px;
	left: 0;
	overflow: hidden;
}

