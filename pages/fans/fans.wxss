
.fans-wrap{
	width: 100vw;
	height: 100vh;
	overflow: hidden;
	background-color: #161823;
}
.fans-nav{
	width: 100%;
	height: 35px;
	position:fixed;
	top: 25px;
	left: 0;
	z-index: 20;
	border-bottom: 2px solid #22242e;
}
.fans-nav-left-icon{
	font-size: 20px;
	font-weight: bold;
	color: #cccdcf;
	-webkit-transform: rotate(180deg);
	        transform: rotate(180deg);
}
.fans-nav-center{
	width: 100%;
	height: 100%;
	font-size: 18px;
	font-weight: bold;
	color: #e8e8e9;
	text-align: center;
}
.fans-content-container{
	width: 100%;
	height: auto;
	overflow: hidden;
	margin-top:60px;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.fans-recommend{
	width: 100%;
	height: auto;
}

