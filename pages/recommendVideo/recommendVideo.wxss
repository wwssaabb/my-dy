
.recommed-video-wrap{
	width: 100vw;
	height: 100vh;
	overflow: hidden;
	z-index: 1;
}
.recommed-video-nav{
	width: 100%;
	height: 35px;
	position: fixed;
	top:35px;
	left: 0;
	z-index: 21;
}
.recommed-video-tab{
	width: 100%;
	height: 50px;
	position: fixed;
	bottom:0;
	left: 0;
	z-index: 21;
}
.recommed-video-content{
	width: 100vw;
	height: 100vh;
	-webkit-border-radius:10px;
	        border-radius:10px;
	z-index: 20;
}
.recommed-video-left{
	position: fixed;
	left: 10px;
	bottom: 50px;
	width: 70%;
	height: auto;
	z-index: 21;
}
.recommed-video-right{
	position: fixed;
	right: 10px;
	bottom: 50px;
	width: 60px;
	height: auto;
	z-index: 21;
}

