
.recommed-video-wrap{
	width: 100%;
	height: 100%;
	background-color: #000;
	overflow: hidden;
	position: relative;
}
.recommed-video-wrap::after{
	position: absolute;
	content: '';
	top: 0;
	left: 0;
	width: 100%;
	height: 20px;
	background-color: #000000;
	z-index: 21;
}
.recommed-video-nav{
	width: 100%;
	height: 30px;
	position: fixed;
	top:25px;
	left: 0;
	z-index: 21;
}
.recommed-video-tab{
	width: 100%;
	height: 50px;
	position: fixed;
	bottom:0;
	left: 0;
	z-index: 21;
}
.recommed-video-content{
	width: 100vw;
	height: 100vh;
	-webkit-border-radius:10px;
	        border-radius:10px;
	z-index: 20;
}
.recommed-video-left{
	position: fixed;
	left: 10px;
	bottom: 50px;
	width: 70%;
	height: auto;
	z-index: 21;
}
.recommed-video-right{
	position: fixed;
	right: 10px;
	bottom: 50px;
	width: 60px;
	height: auto;
	z-index: 21;
}

